{
  "name": "webpack-typescript-babel",
  "version": "1.0.0",
  "description": "Pockey - using lance, babel and webpack",
  "scripts": {
    "start": "node index.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "buildDevServer": "webpack --config webpack.dev.config.js --config-name server --display-modules",
    "buildDevClient": "webpack --config webpack.dev.config.js --config-name client --display-modules",
    "buildDevClientAndServer": "npm run buildDevServer && npm run buildDevClient",
    "buildProdServer": "webpack --config webpack.prod.config.js --config-name server",
    "buildProdClient": "webpack --config webpack.prod.config.js --config-name client"
  },
  "author": {
    "name": "Sandru",
    "email": "sandruandreistefan@gmail.com"
  },
  "main": "dist/bundle.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-typescript": "7.1.0",
    "babel-loader": "^8.0.5",
    "browserify-versionify": "^1.0.6",
    "copy-webpack-plugin": "^5.0.0",
    "event-hooks-webpack-plugin": "^2.1.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "0.5.2",
    "hard-source-webpack-plugin": "^0.13.1",
    "typescript": "3.3.1",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@types/howler": "^2.0.6",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.9.5",
    "@types/node-static": "^0.7.2",
    "@types/p2": "^0.7.32",
    "@types/pg": "^7.4.11",
    "@types/pixi.js": "^4.8.6",
    "@types/query-string": "^6.2.0",
    "@types/socket.io": "^2.1.2",
    "@types/webfontloader": "^1.6.29",
    "babel-preset-env": "^1.7.0",
    "babylonjs": "^3.3.0",
    "engine.io": "^3.3.2",
    "es6-shim": "^0.35.4",
    "gsap": "^2.1.2",
    "howler": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "lance-gg": "^3.1.1",
    "lodash": "^4.17.11",
    "node-static": "^0.7.11",
    "npm": "^6.8.0",
    "pg": "^7.6.1",
    "pixi.js": "^5.0.0-rc.2",
    "query-string": "^6.2.0",
    "socket.io": "^2.2.0",
    "webfontloader": "^1.6.28",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
